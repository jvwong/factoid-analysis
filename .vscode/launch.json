  "version": "0.2.0",
  "configurations": [
    {
      "name": "deltaSub",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/factoid-analysis.js",
      "runtimeVersion": "22.11.0",
      "args": [
        "deltaSub",
        "--input",
        "elife_public_2024.txt",
        "--output",
        "elife_public_2024_deltaSub.csv"
      ],
      "env": {}
    },

    {
      "name": "entities",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/factoid-analysis.js",
      "runtimeVersion": "22.11.0",
      "args": [
        "entities",
        "--input",
        "elife_public_2024.txt",
        "--output",
        "elife_public_2024_entities.csv"
      ],
      "env": {}
    },
  ]
}